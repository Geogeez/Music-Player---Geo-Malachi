let song;

function preload() {
    song = loadSound("28 - SuperBuu.mp3");
}

function setup() {
    // Create the canvas
    createCanvas(innerWidth, innerHeight);
    background(200);
    
    
}

function draw() {

}


function mousePressed() {
    if(song.isPlaying()) {
        song.stop();
        background('red');
      }
      
      
}
        
      
