let song;

function preload() {
    song = loadSound(03 MainMenu.mp3);
}

function setup() {
    // Create the canvas
    createCanvas(innerWidth, innerHeight);
    background(250);
    
    
}

function draw() {

}


function mousePressed() {
    if(song.isPlaying()) {
        song.stop();
        background('red');
      }
      
      
}
  
